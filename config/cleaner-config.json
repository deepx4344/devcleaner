{
  "directoriesToScan": ["."],
  "filePatternToDelete": [
    "**/*.temp",
    "**/*.tmp",
    "**/*.log",
    "**/*.logs",
    "**/*.lock",
    "**/*.locks",
    "**/npm-debug.log*",
    "**/yarn-debug.log*",
    "**/yarn-error.log*",
    "**/*lock.json",
    "**/*.test.ts",
    "**/*.test.js",
    "**/*.spec.js",
    "**/*.cy.js",
    "**/*.map"
  ],
  "folderToDeleteByName": [
    ".DS_Store",
    "__pycache__",
    "temp_cache",
    "logs",
    "temp",
    "temps",
    "log"
  ],
  "fileExclusions": ["index.js", "main.js", "package.json", "package-lock.json", "tsconfig.json"],
  "folderExclusions": ["node_modules", ".git", "dist", "build", ".github"]
}
